{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run translations:update && vite",
    "build": "npm run translations:update && tsc -b && vite build",
    "preview": "vite preview",
    "check:arch": "node ./scripts/check-architecture.cjs",
    "check:code": "npm run compile && npm run format:check && npm run lint",
    "fix:code": "npm run format && npm run lint:fix",
    "arch:tree": "node ./scripts/generate-tree.cjs ./src webappTree.md",
    "compile": "tsc -b",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "biome lint --diagnostic-level=warn ./src",
    "lint:error": "biome lint --diagnostic-level=error ./src",
    "lint:all": "biome lint ./src",
    "lint:fix": "biome lint --diagnostic-level=warn --write ./src",
    "translations:update": "./scripts/update-translations.sh"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "coordo": "github:dataforgoodfr/Coordonnees#main",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.4",
    "react-resizable-panels": "^3.0.6",
    "react-router-dom": "^7.13.0",
    "recharts": "^2.15.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@tailwindcss/vite": "^4.1.18",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/chart.js": "^2.9.41",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "globals": "^17.2.0",
    "jiti": "^2.6.1",
    "prettier": "3.8.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.6.2",
    "vite": "^7.2.4"
  }
}
